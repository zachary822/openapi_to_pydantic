        "/posts/{year}/{month}/": {
            "get": {
                "tags": [
                    "posts"
                ],
                "summary": "Read Month Posts",
                "description": "Get posts for each month",
                "operationId": "read_month_posts_posts__year___month___get",
                "parameters": [
                    {
                        "required": true,
                        "schema": {
                            "title": "Year",
                            "maximum": 9999.0,
                            "minimum": 1.0,
                            "type": "integer"
                        },
                        "name": "year",
                        "in": "path"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Month",
                            "maximum": 12.0,
                            "minimum": 1.0,
                            "type": "integer"
                        },
                        "name": "month",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Read Month Posts Posts  Year   Month   Get",
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Post"
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
